<mvc:View controllerName="SampleSample.controller.home" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Image src="https://www.google.co.in/images/branding/googlelogo/2x/googlelogo_color_272x92dp.png"
					       densityAware="false" 
					       activeSrc="https://static.pexels.com/photos/248797/pexels-photo-248797.jpeg"
					       alt="satish"
					       backgroundPosition="initial"
					       backgroundRepeat="no-repeat"
					       backgroundSize="cover"
					       decorative="true"
					       height="500px"
					       useMap="Fulcrum"
					       width="500px"
					       tap="imageClick"
					       press="imagePress">
					</Image>

				</content>
			</Page>
		</pages>
	</App>
</mvc:View>